<h1>Listing all current Users</h1>

<table>
	<tr>
		<th>Username</th>
		<th>Age</th>
		<th>Email</th>
		<th>Password</th>
	</tr>

<%@users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.age %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to 'Show', user_path(user) %></td>
	</tr>
<% end %>

</table>

<%= link_to 'New User', new_user_path %>
